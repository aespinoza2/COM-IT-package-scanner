#: import NoTransition kivy.uix.screenmanager.NoTransition

WindowManager:
    transition: NoTransition()
    LapWindow:
    DeskWindow:
    MonWindow:
    KeyWindow:
    DockWindow:
    PrintWindow:
    PackSlipScanWindow:
    PackSlipReadWindow:

<LapWindow>
    name: "lapWin"
    canvas.before:
        Color:
            rgba: 0,0,0,.8
        Rectangle:
            pos: self.pos
            size: self.size
    canvas:
        Color:
            rgba: .7,0,0,1
        Rectangle:
            size: 45,self.height
            pos: 0,0

    FloatLayout:
        MDIconButton:
            icon: "laptop"
            user_font_size: "20sp"
            theme_text_color: 'Custom'
            text_color: 1,1,1,1
            pos: 0,root.height-40
            on_release:
                app.root.current = "lapWin"
        MDIconButton:
            icon: "monitor"
            user_font_size: "20sp"
            theme_text_color: 'Custom'
            text_color: 1,1,1,1
            pos: 0,root.height-75
            on_release:
                app.root.current = "monWin"
        MDIconButton:
            icon: "desktop-tower"
            user_font_size: "20sp"
            theme_text_color: 'Custom'
            text_color: 1,1,1,1
            pos: 0,root.height-110
            on_release:
                app.root.current = "deskWin"
        MDIconButton:
            icon: "keyboard-outline"
            user_font_size: "20sp"
            theme_text_color: 'Custom'
            text_color: 1,1,1,1
            pos: 0,root.height-145
            on_release:
                app.root.current = "keyWin"
        MDIconButton:
            icon: "dock-window"
            user_font_size: "20sp"
            theme_text_color: 'Custom'
            text_color: 1,1,1,1
            pos: 0,root.height-180
            on_release:
                app.root.current = "dockWin"
        MDIconButton:
            icon: "printer"
            user_font_size: "20sp"
            theme_text_color: 'Custom'
            text_color: 1,1,1,1
            pos: 0,root.height-215
            on_release:
                app.root.current = "printWin"
        MDIconButton:
            icon: "qrcode-scan"
            user_font_size: "20sp"
            theme_text_color: 'Custom'
            text_color: 1,1,1,1
            pos: 0,root.height-root.height
            on_release:
                app.root.current = "packSlipScanWin"

        MDLabel:
            text: "Laptop"
            font_size: 12
            size_hint_x: None
            width: 40
            color: 1,1,1,1
            bold: "True"
            pos_hint: {"center_x": .5, "center_y": .9}
        MDLabel:
            text: "Locate and Scan Package Barcodes"
            font_size: 12
            #bold: True
            size_hint_x: None
            width: 205
            color: 1,1,1,1
            pos_hint: {"center_x": .5, "center_y": .83}
        MDLabel:
            text: "Tracking"
            font_size: 12
            size_hint_x: None
            width: 50
            color: 1,1,1,1
            pos_hint: {"center_x": .29, "center_y": .75}
        MDTextField:
            id: trackIn
            font_size: "12sp"
            current_hint_text_color: 1, 1, 1, 1
            line_color_normal: 1, 1, 1, 1
            line_color_focus: 1, 1, 1, 1
            multiline: False
            size_hint_x: None
            width: 300
            pos_hint: {"center_x": .5, "center_y": .75}
            on_text:
                app.macProcess()
        MDLabel:
            text: "Order"
            font_size: 12
            size_hint_x: None
            width: 35
            color: 1,1,1,1
            pos_hint: {"center_x": .3, "center_y": .67}
        MDTextField:
            id: orderIn
            font_size: "12sp"
            current_hint_text_color: 1, 1, 1, 1
            line_color_normal: 1, 1, 1, 1
            line_color_focus: 1, 1, 1, 1
            multiline: False
            size_hint_x: None
            width: 300
            pos_hint: {"center_x": .5, "center_y": .67}
            on_text: app.macProcess()
        MDLabel:
            text: "Ticket"
            font_size: 12
            size_hint_x: None
            width: 35
            color: 1,1,1,1
            pos_hint: {"center_x": .3, "center_y": .59}
        MDTextField:
            id: ticketIn
            font_size: "12sp"
            current_hint_text_color: 1, 1, 1, 1
            line_color_normal: 1, 1, 1, 1
            line_color_focus: 1, 1, 1, 1
            multiline: False
            size_hint_x: None
            width: 300
            pos_hint: {"center_x": .5, "center_y": .59}
            on_text:
                app.macProcess()
                app.comHelp()
        MDLabel:
            text: "Serial"
            font_size: 12
            size_hint_x: None
            width: 35
            color: 1,1,1,1
            pos_hint: {"center_x": .3, "center_y": .51}
        MDTextField:
            id: serialIn
            font_size: "12sp"
            current_hint_text_color: 1, 1, 1, 1
            line_color_normal: 1, 1, 1, 1
            line_color_focus: 1, 1, 1, 1
            multiline: False
            size_hint_x: None
            width: 300
            pos_hint: {"center_x": .5, "center_y": .51}
            on_text: app.macProcess()
        MDLabel:
            text: "MAC"
            font_size: 12
            size_hint_x: None
            width: 30
            color: 1,1,1,1
            pos_hint: {"center_x": .3, "center_y": .43}
        MDTextField:
            id: macIn
            font_size: "12sp"
            current_hint_text_color: 1, 1, 1, 1
            line_color_normal: 1, 1, 1, 1
            line_color_focus: 1, 1, 1, 1
            multiline: False
            size_hint_x: None
            width: 300
            pos_hint: {"center_x": .5, "center_y": .43}
            on_text: app.macProcess()
        MDTextField:
            id: tagIn
            text: "COM-"
            font_size: "12sp"
            current_hint_text_color: 1, 1, 1, 1
            line_color_normal: 1, 1, 1, 1
            line_color_focus: 1, 1, 1, 1
            multiline: False
            size_hint_x: None
            width: 300
            pos_hint: {"center_x": .5, "center_y": .34}
            on_text: app.macProcess()
        MDRaisedButton:
            text: "New Item"
            font_size: 12
            pos_hint: {"center_x": .5, "center_y": .2}
            md_bg_color:  .7,0,0,1
            on_release:
                trackIn.text = ''
                orderIn.text = ''
                ticketIn.text = ''
                serialIn.text = ''
                macIn.text = ''
                tagIn.text = 'COM-'
                app.root.current = "lapWin"

